{
	"name": "vite-plugin-webfont-dl",
	"version": "3.1.1",
	"description": "Vite plugin for downloading and injecting webfonts",
	"keywords": [
		"vite",
		"vite-plugin",
		"webfont",
		"webfont-dl",
		"webfont-download",
		"google-fonts",
		"vue",
		"react"
	],
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist/index.js",
		"dist/index.mjs",
		"dist/index.d.ts"
	],
	"scripts": {
		"start": "npm run build -- --watch",
		"build": "tsup src/index.ts --dts --format cjs,esm",
		"lint": "tsc src/index.ts --noEmit --downlevelIteration && eslint \"src/**/*.ts\" --no-fix",
		"lint:fix": "eslint \"src/**/*.ts\" --fix"
	},
	"author": "feat. agency <hello@feat.agency>",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.25"
	},
	"peerDependencies": {
		"vite": "^2 || ^3"
	},
	"devDependencies": {
		"@types/node": "^17.0",
		"@typescript-eslint/eslint-plugin": "^5.0",
		"eslint": "^8.7",
		"rollup": "^2.66",
		"tsup": "^5.11",
		"typescript": "^4.5"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/feat-agency/vite-plugin-webfont-dl.git"
	},
	"bugs": {
		"url": "https://github.com/feat-agency/vite-plugin-webfont-dl/issues"
	},
	"homepage": "https://webfont-dl.feat.agency"
}
